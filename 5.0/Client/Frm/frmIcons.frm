Private Type POINTAPI
        X As Long
        Y As Long
End Type

Private Declare Function GetCursorPos Lib "user32" (lpPoint As POINTAPI) As Long
Private Declare Function CallWindowProc Lib "user32" Alias "CallWindowProcA" (ByVal lpPrevWndFunc As Long, ByVal hWnd As Long, ByVal Msg As Long, ByVal wParam As Long, ByVal lParam As Long) As Long


Private Sub Form_Load()
    Dim myPoint As POINTAPI
    myheight = Me.Height + 100
    GetCursorPos myPoint
    Me.Top = Screen.TwipsPerPixelY * myPoint.Y - myheight
    Me.Left = Screen.TwipsPerPixelX * myPoint.X
End Sub

Private Sub Form_Deactivate()
    Unload Me
End Sub

Private Sub Form_LostFocus()
    Unload Me
End Sub

Private Sub img_Click(Index As Integer)
    If Index <> 27 Then
        frmMain.txtChat.Text = frmMain.txtChat.Text & " " & img(Index).Tag
        frmMain.txtChat.SelStart = Len(frmMain.txtChat)
    Else
        Shell "explorer.exe """ & App.Path & "\Emoticons\index.html" & """", vbMaximizedFocus
    End If
    frmMain.txtChat.SetFocus
    Unload Me
End Sub

Private Sub img_MouseMove(Index As Integer, Button As Integer, Shift As Integer, X As Single, Y As Single)
    For i = 0 To img.UBound
        img(i).BorderStyle = 0
    Next
    img(Index).BorderStyle = 1
End Sub
